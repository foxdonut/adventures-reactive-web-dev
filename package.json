{
  "name": "adventures-reactive-web-dev",
  "version": "1.0.0",
  "description": "Adventures in Reactive Web Development",
  "main": "index.js",
  "scripts": {
    "start:cyclejs": "browserify -o public/generated-app.js client-cyclejs/app.js && node --harmony server/main",
    "watch:cyclejs": "watchify -v -o public/generated-app.js client-cyclejs/app.js",
    "start:react-rxjs": "browserify -o public/generated-app.js client-react-rxjs/app.js && node --harmony server/main",
    "watch:react-rxjs": "watchify -v -o public/generated-app.js client-react-rxjs/app.js",
    "start:react-kefir": "browserify -o public/generated-app.js client-react-kefir/app.js && node --harmony server/main",
    "watch:react-kefir": "watchify -v -o public/generated-app.js client-react-kefir/app.js",
    "start:react-redux": "browserify -o public/generated-app.js client-react-redux/app.js && node --harmony server/main",
    "watch:react-redux": "watchify -v -o public/generated-app.js client-react-redux/app.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/adventures-reactive-web-dev.git"
  },
  "keywords": [
    "cycle.js",
    "example",
    "kefir",
    "react",
    "reactive",
    "rxjs"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/adventures-reactive-web-dev/issues"
  },
  "homepage": "https://github.com/foxdonut/adventures-reactive-web-dev#readme",
  "dependencies": {
    "@cycle/core": "^3.1.0",
    "@cycle/dom": "^5.3.1",
    "@cycle/http": "^4.0.0",
    "axios": "^0.7.0",
    "co-body": "^4.0.0",
    "form-serialize": "^0.6.0",
    "hyperscript-helpers": "^2.0.1",
    "kefir": "^3.1.0",
    "koa": "^1.1.0",
    "koa-route": "^2.4.2",
    "koa-static": "^1.5.1",
    "ramda": "^0.18.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-actions": "^0.8.0",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "rx": "^4.0.6",
    "rx-dom": "^7.0.3",
    "validate.js": "^0.9.0"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "browserify-hmr": "^0.3.1",
    "redux-devtools": "^2.1.5",
    "watchify": "^3.4.0",
    "webpack": "^1.12.9"
  }
}
